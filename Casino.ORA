--Create table ASSIGN
CREATE TABLE ASSIGN (
    STAFF_ID VARCHAR2(10) , 
    TABLE_ID VARCHAR2(10),
    FOREIGN KEY (STAFF_ID) REFERENCES STAFF(STAFF_ID), 
    FOREIGN KEY (TABLE_ID) REFERENCES TABLE_(TABLE_ID),
    CHIP_IN_HAND NUMBER(10),
    JOIN_DATE DATE NOT NULL,
    constraint ASSIGN_pk primary key (STAFF_ID,TABLE_ID) );
/

