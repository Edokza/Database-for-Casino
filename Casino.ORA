--Create table table
CREATE TABLE TABLE_ (
    TABLE_ID VARCHAR2(10) PRIMARY KEY,
    ROOM_ID VARCHAR2(10) NOT NULL, 
    GAME_ID VARCHAR2(10) NOT NULL,
    FOREIGN KEY (ROOM_ID) REFERENCES ROOM(ROOM_ID), 
    FOREIGN KEY (GAME_ID) REFERENCES GAME(GAME_ID)); 
